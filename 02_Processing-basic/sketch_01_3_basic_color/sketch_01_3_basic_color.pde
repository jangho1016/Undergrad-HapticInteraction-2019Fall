background(255);  //set background color(255)
noStroke(); //set no border

// Bright red
fill(255,0,0); //set ellipse color(r(255), g(0), b(0))
ellipse(20,20,16,16); //draw ellipse(position of x(20), position of y(20), weight(16), height(16))

// Dark red
fill(127,0,0); //set ellipse color(r(127), g(0), b(0))
ellipse(40,20,16,16); //draw ellipse(position of x(40), position of y(20), weight(16), height(16))

// Pink (pale red)
fill(255,200,200); //set ellipse color(r(255), g(200), b(200))
ellipse(60,20,16,16); //draw ellipse(position of x(60), position of y(20), weight(16), height(16))
